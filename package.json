{
  "name": "scalable-react-boilerplate",
  "version": "1.0.1",
  "description": "A highly scalable React starter project",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test karma start config/testing/karma.conf.js --single-run",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "test:firefox": "npm run test -- --browsers Firefox",
    "test:safari": "npm run test -- --browsers Safari",
    "test:ie": "npm run test -- --browsers IE",
    "coveralls": "cat ./coverage/lcov/lcov.info | coveralls",
    "build": "`npm bin`/webpack",
    "dev": "`npm bin`/webpack-dev-server",
    "generate": "plop --plopfile config/generators/index.js",
    "generator": "npm run generate",
    "generate:component": "plop --plopfile config/generators/index.js component",
    "generate:container": "plop --plopfile config/generators/index.js container",
    "generate:page": "plop --plopfile config/generators/index.js page",
    "lint": "`npm bin`/eslint . --ext .js --ext .jsx; exit 0",
    "deploy": "NODE_ENV=production `npm bin`/webpack -p",
    "start": "npm run dev",
    "clean": "rm -rf app/dist app/build",
    "setup": "npm install",
    "serve": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RyanCCollins/scalable-react-boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "redux",
    "react",
    "webpack",
    "sass",
    "css modules"
  ],
  "engines": {
    "node": "4.2.4",
    "npm": "3.8.8"
  },
  "author": "Ryan Collins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RyanCCollins/scalable-react-boilerplate/issues"
  },
  "homepage": "https://github.com/RyanCCollins/scalable-react-boilerplate#readme",
  "dependencies": {
    "actions": "^1.3.0",
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "components": "^0.1.0",
    "css-loader": "^0.23.0",
    "expect": "^1.20.2",
    "expect-jsx": "^2.6.0",
    "file-loader": "^0.9.0",
    "foundation-sites": "^6.2.3",
    "grommet": "^0.6.10",
    "history": "^1.14.0",
    "html-webpack-plugin": "^2.7.1",
    "immutable": "^3.7.5",
    "isomorphic-fetch": "^2.2.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "react": "^15.1.0",
    "react-addons-css-transition-group": "^15.2.1",
    "react-dom": "^15.0.1",
    "react-foundation": "^0.6.8",
    "react-intl": "^2.1.3",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-form": "^5.2.5",
    "redux-logger": "^2.6.1",
    "redux-promise-middleware": "^3.2.0",
    "redux-thunk": "^1.0.0",
    "resolve-url-loader": "^1.4.4",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "svg-react-loader": "^0.3.6",
    "webpack": "^1.12.9"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta4",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.4.1",
    "chai-enzyme": "^0.5.0",
    "chai-immutable": "^1.5.3",
    "cross-env": "^2.0.0",
    "enzyme": "^2.4.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.11.3",
    "isparta": "4.0.0",
    "isparta-loader": "2.0.0",
    "jsdom": "^7.2.0",
    "karma": "1.1.1",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-ie-launcher": "1.0.0",
    "karma-mocha": "1.1.1",
    "karma-mocha-reporter": "2.0.4",
    "karma-safari-launcher": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "mocha": "^2.3.4",
    "npm-install-webpack-plugin": "^4.0.3",
    "null-loader": "0.1.1",
    "plop": "1.5.0",
    "react-css-modules": "^3.7.6",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.0.1",
    "webpack-dev-server": "^1.14.0"
  }
}
