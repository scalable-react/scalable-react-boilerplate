{
  "name": "scalable-react-boilerplate",
  "version": "1.1.0",
  "description": "A highly scalable React starter project",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "flow-react-proptypes",
      "transform-flow-strip-types"
    ],
    "env": {
      "development": {
        "plugins": [
          "react-hot-loader/babel"
        ]
      },
      "production": {
        "plugins": [
          [
            "babel-plugin-webpack-alias",
            {
              "config": "./webpack.config.prod.js"
            }
          ]
        ]
      },
      "test": {
        "plugins": [
          [
            "transform-async-to-generator"
          ],
          [
            "transform-es2015-modules-commonjs"
          ],
          [
            "babel-plugin-webpack-alias",
            {
              "config": "./webpack.config.js"
            }
          ]
        ]
      }
    }
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/config/testing/preprocessor.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ]
  },
  "scripts": {
    "t": "npm run test",
    "test:watch": "npm test -- --watch",
    "test": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=test node --harmony_proxies node_modules/.bin/jest",
    "build": "./node_modules/.bin/webpack --config ./webpack.config.prod.js",
    "dev": "node devServer.js",
    "generate": "plop --plopfile config/generators/index.js",
    "generate:component": "plop --plopfile config/generators/index.js component",
    "generate:container": "plop --plopfile config/generators/index.js container",
    "generate:page": "plop --plopfile config/generators/index.js page",
    "lint": "./node_modules/.bin/eslint . --ext .js --ext .jsx",
    "predeploy": "rimraf server/public/*.*",
    "deploy": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack.config.prod.js",
    "start": "npm run dev",
    "clean": "rm -rf app/dist app/build",
    "setup": "npm install",
    "setup:yarn": "yarn",
    "serve:bundle": "cross-env NODE_ENV=production node server.js",
    "postinstall": "npm run deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RyanCCollins/scalable-react-boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "redux",
    "react",
    "webpack",
    "sass",
    "css modules",
    "grommet"
  ],
  "engines": {
    "node": "6.9.4",
    "npm": "3.10.10"
  },
  "author": "Ryan Collins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RyanCCollins/scalable-react-boilerplate/issues"
  },
  "homepage": "https://github.com/RyanCCollins/scalable-react-boilerplate#readme",
  "dependencies": {
    "apollo-client": "0.10.1",
    "babel-plugin-flow-react-proptypes": "^0.21.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "cross-env": "^3.1.4",
    "enzyme": "^2.4.1",
    "enzyme-to-json": "^1.0.3",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-flowtype-errors": "^3.0.0",
    "expect": "^1.20.2",
    "express": "^4.14.0",
    "flow-bin": "^0.42.0",
    "functional-components": "0.0.5",
    "graphql-tag": "^1.1.2",
    "grommet": "1.3.4",
    "history": "^4.5.1",
    "immutable": "^3.7.5",
    "isomorphic-fetch": "^2.2.0",
    "jest": "19.0.2",
    "jest-cli": "19.0.2",
    "json-loader": "^0.5.4",
    "markdown-loader": "^2.0.0",
    "minimist": "^1.2.0",
    "morgan": "^1.7.0",
    "node-env-file": "^0.1.8",
    "offline-plugin": "^4.6.1",
    "react": "^15.1.0",
    "react-addons-css-transition-group": "^15.2.1",
    "react-addons-test-utils": "^15.3.2",
    "react-addons-update": "^15.3.2",
    "react-apollo": "0.13.3",
    "react-css-modules": "^4.1.0",
    "react-dom": "^15.0.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-intl": "^2.1.3",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-auth-wrapper": "^1.0.0",
    "redux-form": "^6.5.0",
    "redux-logger": "^2.6.1",
    "redux-mock-store": "^1.1.4",
    "redux-thunk": "^2.2.0",
    "reselect": "^2.5.4",
    "serialize-javascript": "^1.3.0",
    "sinon": "2.1.0",
    "styled-components": "latest",
    "svg-react-loader": "^0.3.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-core": "^6.3.15",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-webpack-alias": "^2.1.1",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-polyfill": "latest",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.16.3",
    "babel-runtime": "^6.11.6",
    "css-loader": "^0.27.3",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-graphql": "^0.7.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "expect-jsx": "^3.0.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.7.1",
    "node-sass": "^4.5.0",
    "npm-install-webpack-plugin": "^4.0.3",
    "null-loader": "0.1.1",
    "plop": "1.7.4",
    "postcss-loader": "^1.1.0",
    "precss": "^1.4.0",
    "redux-devtools": "^3.0.1",
    "resolve-url-loader": "^2.0.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.16.0",
    "webpack": "2.3.1",
    "webpack-dev-server": "2.4.2",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-manifest-plugin": "^1.1.0"
  }
}
